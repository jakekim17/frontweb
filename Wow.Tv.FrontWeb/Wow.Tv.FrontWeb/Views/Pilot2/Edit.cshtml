@model Wow.Tv.Middle.Model.Db49.wowtv.TAB_BOARD


@{
    Layout = null;
    string rootUrl = AppState["RootUrl"].ToString();
}

<script src="@rootUrl/Scripts/Pilot2/Edit.js?date=20170721110001"></script>


<form id="frmSave">
    <input type="hidden" name="seq" value="@Model.SEQ" />
    <table>
        <tr>
            <td>
                제목
            </td>
            <td>
                <input type="text" name="TITLE" value="@Model.TITLE" />
            </td>
        </tr>
        <tr>
            <td>
                내용
            </td>
            <td style="width:850px;">
                <textarea id="editor" name="CONTENT" style="width:800px; height:300px;">@Model.CONTENT</textarea>
            </td>
        </tr>
        <tr>
            <td>
                <a href="#" id="btnSave">저장</a>
                <a href="#" id="btnClose">닫기</a>
            </td>
        </tr>
    </table>
</form>



