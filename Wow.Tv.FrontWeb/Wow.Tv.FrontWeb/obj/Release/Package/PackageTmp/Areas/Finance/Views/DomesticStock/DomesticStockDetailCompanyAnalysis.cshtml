
@{
    ViewBag.Title = "DomesticStockDetailCompanyAnalysis";
    Layout = null;
    string wowTvScript = AppState["WowTvScript"].ToString();
    string arjCode = Request["arjCode"];
    if (!String.IsNullOrEmpty(arjCode))
    {
        arjCode = "A" + arjCode.Substring(0, 5) + "0";
    }
}
<script src="@wowTvScript/Finance/DomesticStock/DomesticDetailCompanyAnalysis.js"></script>

<!-- 기업분석 -->
<div class="section-content">

    <div class="tab-type2" id="companyAnalysisTab">
        <!-- 171030 tab-type2로 class 변경 -->
        <ul class="col6">
            <!-- 171030 col6로 class 변경 -->
            <li class="on"><a href="#none" id="company-status">기업현황</a></li>
            <li><a href="#none" id="company-outline">기업개요</a></li>
            <li><a href="#none" id="financial-state">재무제표</a></li>
            <li><a href="#none" id="financial-ratio">재무비율</a></li>
            <li><a href="#none" id="invest-indicate">투자지표</a></li>
            <li><a href="#none" id="compete">경쟁사비교</a></li>
        </ul>
    </div>
    <input type="hidden" id="companyAnalysisStockCode" value="@arjCode" />
    <iframe src="" frameborder="0" width="100%" id="cpAnalysisIframe" style="height: 1523px;"></iframe>

</div>
<!-- //기업분석 -->


