
@{
    ViewBag.Title = "Index";

    string rootUrl = AppState["RootUrl"].ToString();
}


@section scripts{

    <script>
        $(document).ready(function () {

            $("#btnSend").click(function () {
                var form = $('#frm')[0];
                var formData = new FormData(form);
                $.ajax({
                    url: './Upload2',
                    data: formData,
                    processData: false,
                    contentType: false,
                    type: 'POST',
                    success: function (result) {
                        alert("Ok");
                    }
                });

                return false;
            });


        });
    </script>
}

