@if (Model.ListData.Count > 0)
{
    <div class="list-type-thumnail small bxslider">
        <ul>
            @foreach (var item in Model.ListData)
            {
            <li>
                <a href="#none">
                    <img src="@Html.DomesticStockDetailRightConsultImgUrl(item.ARTICLEID, item.EDITFOLDER, item.IMAGE_S)" alt="" width="250" height="161" onclick="PopupPlayVod('@item.VODNUM')" onerror="this.src='@wowTvStyle/images/common/no_image_16m.jpg'">
                    <span class="title" onclick="PopupPlayVod('@item.VODNUM')">@item.SUBJECT</span>
                    <span class="date">@item.DISP_DATE</span>
                </a>

            </li>
            }
        </ul>

    </div>
}
else
{
    <div class="list-type-thumnail small">
        <ul>
            <li>데이터가 없습니다.</li>
        </ul>
    </div>
}



