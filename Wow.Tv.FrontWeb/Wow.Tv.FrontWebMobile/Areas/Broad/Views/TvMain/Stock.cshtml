@using Wow.Tv.Middle.Model.Common
@using Wow.Tv.Middle.Model.Db49.editVOD

@model List<tblStockClass>


@{
    Layout = null;

    string wowTvStyle = AppState["WowTvStyle"].ToString();
    string wowTvScript = AppState["WowTvScript"].ToString();

}



<div class="list-program type2">
    <ul>
        @foreach (var item in Model)
        {
            <li>
                <div class="program-area">
                    <div class="photo-area">
                        <a href="#" onclick="return TvMainIndex.OpenVod(@item.vodNum);">
                            <img src="http://vodimg.wowtv.co.kr/@item.EditFolder.Trim()/@item.ImageS" alt="">
                        </a>
                    </div>
                    <div class="info-area">
                        <strong class="tit"><a href="#" onclick="return TvMainIndex.OpenVod(@item.vodNum);">@item.StockName<span class="font-color06 num">(@item.StockCode)</span></a></strong>
                        <div class="btn-area news-util">
                            <a href="#" onclick="return TvMainIndex.OpenStockItem('@item.StockCode.Replace("A","")');" class="btn type04">종목보기</a>
                        </div>
                    </div>
                </div>
            </li>
        }



