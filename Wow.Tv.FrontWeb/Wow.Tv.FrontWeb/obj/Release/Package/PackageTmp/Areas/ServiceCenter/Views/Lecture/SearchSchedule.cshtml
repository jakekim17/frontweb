
@{
    Layout = null;
}

<!-- box area -->
<div class="box-bg-gray">
    <!-- seminar detail -->
    <div class="seminar-detail">
        <p class="cal">일시<span class="date">@Model.LECTURES_DATE.Replace('-','.') (@Model.LECTURES_TIME)</span></p>
        <p class="lecturer">강사</p>
        <!-- lecturer detail -->
        <div class="lecturer-detail">
            <div class="inner">
                @foreach(var item in Model.LecturerList)
                {
                    if (item.LECTURER_TYPE == "G")
                    {
                        <div class="ppl nolink">
                            <p class="name">@item.LECTURER</p>
                        </div>
                    }
                    else if (item.LECTURER_TYPE == "P")
                    {
                        <div class="ppl">
                            <p class="name">@item.NickName</p>
                            <p>
                                <span class="link"><a href="">파트너 홈</a></span>
                                <span class="space"></span><span class="link">
                                <a href="">카페가기</a></span>
                            </p>
                        </div>
                    }
                }
            </div>
        </div>
        <!-- // lecturer detail -->
        <!-- view area -->
        <div class="seminar-view-txt">
          @Model.ETC
        </div>
        <!-- // view area -->
    </div>
    <!-- // seminar detail -->
    <div class="inner-right">
        <span class="btn-type1"><button>신청하기</button></span>
    </div>
</div>
<!-- // box area -->

<hr class="division3 no-border">
<p class="con-tit">위치보기</p>

<!-- map area -->
<div class="box-map">
    <div class="map-view" id="map"><img src="../../images/customer/img_map_dummy.png" alt=""></div>
    <div class="map-address">@Model.PLACE</div>
</div>
<!-- map area -->

