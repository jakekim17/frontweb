@model Wow.Tv.Middle.Model.Db49.wowtv.TAB_BOARD


@{
    string rootUrl = AppState["RootUrl"].ToString();
}



@section scripts{
    <script src="@rootUrl/Scripts/Pilot/Detail.js"></script>
}


<form id="frmSearch">
    <input type="hidden" id="seq" name="seq" value="@Model.SEQ" />
    <input type="hidden" id="title" name="title" value="@ViewBag.Condition.Title" />
    <input type="hidden" id="pageSize" name="pageSize" value="@ViewBag.Condition.PageSize" />
    <input type="hidden" id="currentIndex" name="currentIndex" value="@ViewBag.Condition.CurrentIndex" />
</form>



<table>
    <tr>
        <td>
            제목 
        </td>
        <td>
            @Model.TITLE
        </td>
    </tr>
    <tr>
        <td>
            내용
        </td>
        <td>
            @MvcHtmlString.Create(Model.CONTENT)
        </td>
    </tr>
    <tr>
        <td>
            <a href="#" id="btnList">목록</a> 
            <a href="#" id="btnModify">수정</a>
            <a href="#" id="btnDelete">삭제</a>
        </td>
    </tr>
</table>


