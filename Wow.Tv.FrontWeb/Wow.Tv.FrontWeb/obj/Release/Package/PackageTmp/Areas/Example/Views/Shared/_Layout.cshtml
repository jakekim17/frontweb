@{
    //string rootUrl = AppState["RootUrl"].ToString();
    string wowTvScript = AppState["WowTvScript"].ToString();
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>한경TV 프로젝트</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script type="text/javascript" src="@wowTvScript/jquery-1.10.2.js"></script>
    <script src="@wowTvScript/kakao.story.min.js"></script>
    @*<link href="@rootUrl/Content/css/jqueryui/jquery-ui.css" rel="stylesheet" />

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    <script src="@rootUrl/Scripts/jquery-ui.js"></script>
    @*<script src="@Url.Content($"{rootUrl}/Scripts/jquery.blockUI.js")"></script>
    <script src="@Url.Content($"{rootUrl}/Scripts/paging.js?date=20170706102000")"></script>*@
    @*<script src="@Url.Content($"{rootUrl}/Scripts/common.js")"></script>*@
    @RenderSection("scripts", required: false)

    @if (IsSectionDefined("ScriptImport"))
    {
        @RenderSection("ScriptImport", false)
    }
    else //다른 것이 생기면 추가
    {
    }
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="/Example/LazyLoad/Index">LazyLoad</a></li>
                    <li><a href="/Example/MagnificPopup/Index">Magnific Popup</a></li>
                    <li><a href="/Example/Chosen/Index">Chosen</a></li>
                    <li><a href="/Example/FileUpload/Index">File Upload</a></li>
                    <li><a href="/Example/CaptCha/Index">CAPTCHA</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p></p>
        </footer>
    </div>


</body>
</html>
