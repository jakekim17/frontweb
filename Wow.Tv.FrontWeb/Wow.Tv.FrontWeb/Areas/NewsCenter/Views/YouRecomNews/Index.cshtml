@{ 
    Layout = null;

    List<string> keyWordList = ViewBag.KeyWordList;
    string newsListTag = ViewBag.NewsListTag;
}

<h2 class="title">당신에게 맞는 추천 뉴스</h2>
<!-- 태그 선택 -->
<div class="box-select-tags">
    @foreach (var item in keyWordList)
    {
        <span class="btn-tag on devNewsReadKeyWord" onclick="return NewsReadSearchApi.KeyWordToggleChange(this);"><button>@item</button></span>
    }
</div>
<!-- //태그 선택 -->
<div class="contain-news-type2">
    <ul id="ulNewsReadKeyWordNewsList">@MvcHtmlString.Create(newsListTag)</ul>
</div>

